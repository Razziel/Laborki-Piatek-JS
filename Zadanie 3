class Main {
  public static void main(String[] args) {
    int[] array = {3,2,14,1,2,3,6,3,3,7,6,6,6};

    for(int i = 0; i < array.length-2; i++){
      if(array[i] == 1){
        if(array[i+1] == 2){
          if(array[i+2] == 3){
            System.out.println("true");
          }
        }
      }
    }
  }
}
